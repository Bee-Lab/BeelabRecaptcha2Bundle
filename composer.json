{
    "name": "beelab/recaptcha2-bundle",
    "type": "symfony-bundle",
    "description": "Provide Google Recaptch2 form type",
    "keywords": ["Symfony", "Form", "Recaptcha"],
    "homepage": "https://github.com/bee-lab/",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": "^7.1",
        "google/recaptcha": "^1.2",
        "symfony/config": "^3.4|^4.1",
        "symfony/dependency-injection": "^3.4|^4.1",
        "symfony/form": "^3.4|^4.1",
        "symfony/http-kernel": "^3.4|^4.1",
        "symfony/validator": "^3.4|^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "symfony/phpunit-bridge": "^4.1"
    },
    "suggest": {
        "symfony/twig-bundle": "To render widget"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Beelab\\Recaptcha2Bundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Beelab\\Recaptcha2Bundle\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
