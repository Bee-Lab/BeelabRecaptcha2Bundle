{
    "name": "beelab/recaptcha2-bundle",
    "type": "symfony-bundle",
    "description": "Provide Google Recaptch2 form type ",
    "keywords": ["Symfony", "Form", "Recaptcha"],
    "homepage": "https://bee-lab.net",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": ">=7.0",
        "google/recaptcha": "^1.1",
        "symfony/config": "^2.8.12|^3.2",
        "symfony/dependency-injection": "^2.8.12|^3.2",
        "symfony/form": "^2.8|^3.2",
        "symfony/http-kernel": "^2.8|^3.2",
        "symfony/validator": "^2.8|^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/phpunit-bridge": "^2.8|^3.2"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Beelab\\Recaptcha2Bundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
