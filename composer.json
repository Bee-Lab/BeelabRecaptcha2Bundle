{
    "name": "beelab/recaptcha2-bundle",
    "type": "symfony-bundle",
    "description": "Provide Google Recaptch2 form type ",
    "keywords": ["Symfony", "Form", "Recaptcha"],
    "homepage": "https://bee-lab.net",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "~2.7|~3.0",
        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/form": "~2.7|~3.0",
        "symfony/validator": "~2.7|~3.0",
        "google/recaptcha": "^1.1"
    },
    "require-dev": {
        "symfony/expression-language": "~2.7|~3.0",
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "phpunit/phpunit": "^4.8"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Beelab\\Recaptcha2Bundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
